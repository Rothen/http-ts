  "description": "A Typescript HTTP server wrapper with express.",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "dependencies": {
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "http": "^0.0.0",
    "https": "^1.0.0",
    "jsonwebtoken": "^8.5.1",
    "request": "^2.88.2",
    "rxjs": "^6.6.2",
    "url": "^0.11.0",
    "winston": "^3.3.3",
    "ws": "^7.3.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alkocats/http-ts.git"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/chai": "^4.2.12",
    "@types/express": "^4.17.7",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.6.0",
    "@types/winston": "^2.4.4",
    "chai": "^4.2.0",
    "coveralls": "^3.1.0",
    "mocha": "^8.1.1",
    "nodemon": "^2.0.4",
    "nyc": "^15.1.0",
    "ts-node": "8.10.2",
    "tsc": "1.20150623.0",
    "tsconfig-lint": "^0.12.0",
    "tslint": "^6.1.3",
    "typedoc": "^0.18.0",
    "typescript": "4.0.2"
  },
  "scripts": {
    "build": "tsc && npm run lint",
    "dev": "tsc --watch & nodemon dev",
