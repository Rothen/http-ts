  "description": "A Typescript HTTP server wrapper with express.",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "dependencies": {
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "http": "^0.0.0",
    "https": "^1.0.0",
    "jsonwebtoken": "^8.5.1",
    "request": "^2.88.0",
    "rxjs": "^6.5.2",
    "url": "^0.11.0",
    "winston": "^3.2.1",
    "ws": "^7.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alkocats/http-ts.git"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/chai": "^4.1.7",
    "@types/express": "^4.16.1",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/mocha": "^5.2.6",
    "@types/node": "^12.0.3",
    "@types/winston": "^2.4.4",
    "chai": "^4.2.0",
    "coveralls": "^3.0.2",
    "mocha": "^6.1.4",
    "nodemon": "^1.19.1",
    "nyc": "^14.1.1",
    "ts-node": "8.2.0",
    "tsc": "1.20150623.0",
    "tsconfig-lint": "^0.12.0",
    "tslint": "^5.16.0",
    "typedoc": "^0.14.2",
    "typescript": "3.4.5"
  },
  "scripts": {
    "build": "tsc && npm run lint",
    "dev": "tsc --watch & nodemon dev",
