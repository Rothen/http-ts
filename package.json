{
  "name": "@alkocats/http-ts",
  "version": "0.0.1",
  "description": "A Typescript HTTP server wrapper with express.",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "dependencies": {
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "http": "^0.0.0",
    "https": "^1.0.0",
    "npm": "^6.9.0",
    "request": "^2.88.0",
    "rxjs": "^6.4.0",
    "url": "^0.11.0",
    "ws": "^6.2.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Rothen/http-ts.git"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/express": "^4.16.1",
    "@types/mocha": "^5.2.6",
    "@types/node": "^11.13.0",
    "chai": "^4.2.0",
    "coveralls": "^3.0.2",
    "mocha": "^6.0.0",
    "nodemon": "^1.18.10",
    "nyc": "^13.3.0",
    "ts-node": "8.0.3",
    "tsc": "1.20150623.0",
    "tsconfig-lint": "^0.12.0",
    "tslint": "^5.15.0",
    "typedoc": "^0.14.2",
    "typescript": "3.4.1"
  },
  "scripts": {
    "build": "tsc && npm run lint",
    "dev": "tsc --watch & nodemon dev",
